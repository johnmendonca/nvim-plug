{
  "languageserver": {
    "hoon-language-server": {
      "command": "hoon-language-server",
      "args": ["-p", "8080", "-s", "let", "-c", "maltem-ragryx-ravlyn-nopwyx"],
      "filetypes": ["hoon"]
    },
    "sorbet": {
      "command": "srb",
      "args": ["tc", "--typed", "true", "--enable-all-experimental-lsp-features", "--lsp", "--disable-watchman"],
      "filetypes": ["ruby"],
      "rootPatterns": ["sorbet/config"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.asdf/shims/black"
}
